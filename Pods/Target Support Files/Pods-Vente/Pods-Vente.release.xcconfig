OTHER_LDFLAGS = $(inherited) -framework "AFNetworking" -framework "BDBOAuth1Manager" -framework "Bolts" -framework "ChameleonFramework" -framework "CocoaLumberjack" -framework "MBProgressHUD" -framework "Parse" -framework "PubNub"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Vente
PODS_ROOT = ${SRCROOT}/Pods